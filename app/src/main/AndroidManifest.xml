<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nicholas.wwtpApplication" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.nicholas.wwtpApplication.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.nicholas.wwtpApplication.CreditActivity"
            android:label="@string/title_activity_credit" >
        </activity>
        <activity
            android:name="com.example.nicholas.wwtpApplication.InformationActivity"
            android:label="@string/title_activity_information" >
        </activity>
        <activity
            android:name="com.example.nicholas.wwtpApplication.ModifierActivity"
            android:label="@string/title_activity_modifier" >
        </activity>
        <activity
            android:name="com.example.nicholas.wwtpApplication.Data"
            android:label="@string/title_activity_data" >
        </activity>
        <activity
            android:name="com.example.nicholas.wwtpApplication.manAutoActivity"
            android:label="@string/title_activity_man_auto" >
        </activity>
        <activity
            android:name="com.example.nicholas.wwtpApplication.manualActivity"
            android:label="@string/title_activity_manual" >
        </activity>
        <activity
            android:name="com.example.nicholas.wwtpApplication.AutomaticActivity"
            android:label="@string/title_activity_automatic" >
        </activity>
    </application>

</manifest>
